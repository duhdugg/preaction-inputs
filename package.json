{
  "name": "@preaction/inputs",
  "version": "3.0.0-alpha.8",
  "main": "dist/preaction-inputs.cjs.js",
  "module": "dist/preaction-inputs.esm.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "rollup -c",
    "build-doc": "styleguidist build",
    "doc": "styleguidist server",
    "test": "prettier -c src/* *.js package.json && eslint *.js src/*.j* && react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    }
  },
  "browserslist": {
    "development": "defaults and supports es6-module",
    "production": "defaults"
  },
  "dependencies": {
    "@loadable/component": "^5.13.1",
    "promise-timeout": "^1.3.0",
    "prop-types": "^15.7.2",
    "react-quill": "^1.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.1.3",
    "bootstrap": "^4.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "react-styleguidist": "^11.0.8",
    "rollup": "^2.23.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-visualizer": "^4.0.4"
  },
  "peerDependencies": {
    "bootstrap": "^4.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "author": "Doug Elkin <doug@dougelkin.com> (https://dougelkin.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:duhdugg/preaction-inputs.git"
